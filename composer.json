{
  "name": "lunr/ticks.phpmailer",
  "description": "Lunr.Ticks.PHPMailer is a wrapper around PHPMailer to gather analytics",
  "keywords": [ "analytics", "email" ],
  "homepage": "https://github.com/lunr-php/lunr.ticks.phpmailer",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "phpmailer/phpmailer": "~6.9",
    "lunr/ticks": "~0.12.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=10.5 <11.0",
    "lunr/halo": "~0.11.0",
    "mockery/mockery": "~1.5",
    "ext-xdebug": "~3.1"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {"Lunr\\": "src/Lunr/"}
  }
}
